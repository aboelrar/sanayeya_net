<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.Scenario_main_screen.Controller.Secnario_Categories.twentyfour.Scenario_map_repair_car.controller.map_repair_location">
    <fragment xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:map="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.carsyalla.admin.mapss.MapsActivity" />
    <ImageView
        android:tint="@color/f_color"
        android:layout_width="@dimen/_15sdp"
        android:layout_height="@dimen/_15sdp"
        android:layout_centerInParent="true"
        android:layout_alignParentEnd="true"
        android:src="@drawable/map_marker"
        android:layout_marginRight="@dimen/_30sdp"/>

     <androidx.recyclerview.widget.RecyclerView
         android:layout_width="match_parent"
         android:layout_height="@dimen/_180sdp"
         android:layout_alignParentBottom="true"
         android:layout_marginLeft="@dimen/_10sdp"
         android:layout_marginBottom="@dimen/_5sdp"
         android:id="@+id/workshop_list"/>
</RelativeLayout>